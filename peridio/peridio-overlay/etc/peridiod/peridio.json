{
  "version": 1,
  "remote_shell": true,
  "remote_access_tunnels": {
    "enabled": true,
    "service_ports": [
        22
    ],
    "persistent_keepalive": 25
  },
  "node": {
    "key_pair_source": "uboot-env",
    "key_pair_config": {
      "private_key": "avocado_device_key",
      "certificate": "avocado_device_cert"
    }
  }
}
